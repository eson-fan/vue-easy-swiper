{
  "name": "vue-easy-swiper",
  "version": "1.0.1",
  "description": "vue swiper on PC",
  "main": "dist/vue-easy-swiper.umd.min.js",
  "private": false,
  "scripts": {
    "release": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "pb": "npm run release && npm run build && npm publish --registry=http://registry.npmjs.org",
    "build": "vue-cli-service build --target lib --name vue-easy-swiper package/src/swiper.vue",
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "js",
    "Vue",
    "swiper",
    "plugin"
  ],
  "author": "esonfan",
  "license": "ISC",
  "files": [
    "dist/vue-easy-swiper.umd.min.js",
    "package.json",
    "CHANGELOG.md",
    "screenshot",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eson-fan/vue-easy-swiper"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "standard-version": "^9.3.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
